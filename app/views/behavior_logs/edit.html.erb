
 <h1>Edit Behavior Log</h1>
<%= form_for ([@behavior_log.user, @behavior_log]) do |f| %>

  
  <%= f.hidden_field :user_id%> 
    <%= f.label :date %>
    <%= f.date_field :date %>

    <%= f.label :time %>
    <%= f.time_field :time%>
    <br>
    <br> 
    <%= f.label :location %>
    <%= f.text_field :location%>
    <br>
    <br> 
    <%= f.label :before_behavior %>
    <%= f.text_field :before_behavior%>
    <br>
    <br> 
    <%= f.label :behavior_content %>
    <%= f.text_area :behavior_content%>
    <br>
    <br>    
    <%= f.label :outcome %>
    <%= f.text_field :outcome%>
    <br>
    <br> 
  <%= f.submit "Update Behavior Log"%>
<% end %>